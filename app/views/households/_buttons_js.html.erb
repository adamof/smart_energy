<%= javascript_tag do %>
  $(function(){
    var description = $(".row#description"),
        survey = $(".row#survey iframe"),
        graph = $("#graph");

    $("#proceed_eval").click(function(argument) {
      description.slideUp("slow");
      survey.attr("height", 260);
      graph.slideDown("slow");
      $("#proceed_eval").hide();
    });

    $("#proceed_final").click(function(argument) {
      survey.attr("height", 1000);
      graph.slideUp("slow");
      $("#proceed_eval").show();
    });

    // graph.show();
    // description.hide();
    // survey.hide();

  });
<% end %>